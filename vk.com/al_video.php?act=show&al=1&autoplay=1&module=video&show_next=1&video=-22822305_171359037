cur.addMeVideo = function() {
  if (isVisible(box.progress)) return;

  var toReplace = [];
  for (var i = 0; i < arguments.length; ++i) {
    toReplace.push(arguments[i]);
  }
  cur.reloadAfterClose = true;
  ajax.post('video.php', {act: 'a_add', video: '-22822305_171359037', hash: '2abcf5f47d03eb0307', from: cur.section}, {
    onDone: function() {
      for (var i = 0; i < toReplace.length; ++i) {
        var el = ge(toReplace[i]);
        if (el) {
          if (i < arguments.length) {
            el.parentNode.innerHTML = arguments[i];
          } else {
            hide(el);
          }
        }
      }
    },
    showProgress: box.showProgress,
    hideProgress: box.hideProgress
  });
}

