if (_n) {
  function pauseVideo() {
    var player = ge('video_player') || window.html5video;
    if (cur.pinnedVideo && player && player.isTouchedByUser && !player.isTouchedByUser()) return;
    if (player && player.playVideo) {
      player.playVideo(false);
    }
  }

  _n.addRecvClbk('audio_start', 'video', pauseVideo);
  _n.addRecvClbk('video_start', 'video', pauseVideo);
  _n.addRecvClbk('videocall_start', 'video', pauseVideo);
}
