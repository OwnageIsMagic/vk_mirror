window.changeCanvasSize = function(w, h) {
  if (typeof(mvcur) != 'undefined' && mvcur.changeCanvasSize) {
    mvcur.changeCanvasSize();
  }
  if (!browser.opera || intval(browser.version) >= 10) {
    hide('video_background-22822305_171359037');
    setStyle(ge('video_player'), {width: w, height: h});
    setStyle(ge('video_box_wrap-22822305_171359037'), {width: w, height: h});
    if (box) {
      box.setOptions({width: w + 22});
    }
  }
};

