var isInline = 0;
var playerWrap = ge('video_box_wrap-22822305_171359037');

addClass(playerWrap, 'hidden');

onLoaded(function() {
  checkMp4(function(canPlay) {
    removeClass(playerWrap, 'hidden');
    if (canPlay && vars.no_flv || vars.live) {
      loadHtml5Player();
    } else if (browser.flash > 10) {
      loadFlashPlayer();
    }
  });
});

function loadHtml5Player() {
  var needReappend = isInline || vars.video_ext || !mvcur.player || mvcur.player.el !== domFC(ge('video_player'));
  if (needReappend) {
    val(playerWrap, '');
    var container = ce('div', {id: 'video_player'});
    attr(container, 'preventhide', 1);
    playerWrap.appendChild(container);
  }

  var staticFiles = ['videoplayer.js', 'videoplayer.css'];
  if (vars.hls) {
    staticFiles.push('hls.min.js');
  }
  if (vars.live_candy) {
    staticFiles.push('candy.min.js');
  }
  stManager.add(staticFiles, function() {
    if (!ge('video_box_wrap-22822305_171359037')) return;

    if (!isInline && window.mvcur && mvcur.player) {
      var player = mvcur.player;
      player.initVideo(vars);
    } else {
      var player = new VideoPlayer(vars);
      if (isInline) {
        cur.videoInlinePlayer = player;
      } else if (vars.video_ext) {
        cur.player = player;
      } else {
        mvcur.player = player;
      }
    }

    if (needReappend) {
      container.appendChild(player.el);
    }
  });
}

function loadFlashPlayer() {
  var opts = {
    url: '/swf/video.swf?156',
    id: 'video_player',
    width: '100%',
    height: '100%',
    version: 11,
    preventhide: 1
  };

  var params = {
    allowfullscreen: 'true',
    allowscriptaccess: 'always',
    bgcolor: '#000000',
    wmode: 'opaque'
  };

  val(playerWrap, '');
  renderFlash(playerWrap, opts, params, vars);
}

if (window.Notifier) {
  function pauseVideo() {
    var player = cur.videoInlinePlayer || window.mvcur && mvcur.player || ge('video_player');
    if (player && (player.isFromAutoplay && player.isFromAutoplay() || cur.pinnedVideo)) {
      if (player.isTouchedByUser && !player.isTouchedByUser()) return;
    }
    if (window.Videoview) {
      Videoview.togglePlay(false);
    }
  }

  Notifier.addRecvClbk('audio_start', 'video', pauseVideo);
  Notifier.addRecvClbk('video_start', 'video', pauseVideo);
  Notifier.addRecvClbk('videocall_start', 'video', pauseVideo);
}
